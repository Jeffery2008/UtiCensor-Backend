{
    "name": "uticensor/backend",
    "description": "UtiCensor Network Activity Auditing System Backend",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Manus Team",
            "email": "support@manus.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mysqli": "*",
        "firebase/php-jwt": "^6.0",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
